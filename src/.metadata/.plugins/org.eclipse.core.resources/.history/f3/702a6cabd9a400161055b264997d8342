import java.util.Scanner;
public class exercici31prova {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	int x, y, p;
	
		Scanner entrada;
		entrada = new Scanner(System.in);
		
		System.out.println("Introdiu el primer numero");
		x = entrada.nextInt();
		System.out.println("Introdiu el primer numero");
		y = entrada.nextInt();
		System.out.println("Introdiu el primer numero");
		p = entrada.nextInt();
		
		if (x>y && x>p && p>y) { System.out.println(x+" , "+ p +" , "+y);
			
		}

	}
}
