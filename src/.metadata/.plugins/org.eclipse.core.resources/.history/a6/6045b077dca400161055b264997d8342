import java.util.Scanner;
public class exercici31prova {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	int x, y, p;
	
		Scanner entrada;
		entrada = new Scanner(System.in);
		
		System.out.println("Introdiu el primer numero");
		x = entrada.nextInt();
		System.out.println("Introdiu el primer numero");
		y = entrada.nextInt();
		System.out.println("Introdiu el primer numero");
		p = entrada.nextInt();
		
		if (x>=y && x>=p && p>=y) { System.out.println(x+" , "+ p +" , "+y);
			
		}
		if (x>=y && x>=p && p<=y) { System.out.println(x+" , "+ y +" , "+p);
				
		}
		if (y>=x & y>=p && x>=p) { System.out.println(y+" , "+ x +" , "+p);
		
		}
		if (y>=x & y>=p && p>=x) { System.out.println(y+" , "+ p +" , "+x);
		
		}
		if (p>=x & p>=y && x>=y) { System.out.println(p+" , "+ x +" , "+y);
		
		}
		if (p>=x & p>=y && y>=y) { System.out.println(y+" , "+ y +" , "+x);
		
		}

	}
}
